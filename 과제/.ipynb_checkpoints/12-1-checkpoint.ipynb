   "source": [
    "y = [10, 20, 30, 40, 50, 60]\n",
    "y_hat = [12, 23, 25, 46, 54, 61]\n",
    "\n",
    "# 절대 오차의 평균\n",
    "mae = sum(abs(a - b) for a, b in zip(y, y_hat)) / len(y)\n",
    "print(\"평균 절대 오차(MAE):\", mae)"
   ]
